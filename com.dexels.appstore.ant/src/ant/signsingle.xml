<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE project>
<project default="signsingle">
	<target name="signsingle" >
		<signjar  keystore="${keystore}" keypass="${keypass}" storepass="${storepass}"  alias="${alias}" jar="${jarfile}" tsaurl="${tsaurl}">
		</signjar>
		<echo message="File ${jarfile} signed"/>
	</target>
</project>