<transform name="Person">
	<rename from="personid" to="PersonId"/>
	<rename from="lastname" to="LastName"/>
	<remove name="countryofbirth"/>
	<rename from="firstname" to="FirstName"/>
	<rename from="initials" to="Initials"/>
	<rename from="infix" to="Infix"/>
	<rename from="dateofbirth" to="DateOfBirth"/>
	<rename from="dateofpassing" to="DateOfPassing"/>
	<rename from="privacy_level" to="PrivacyLevel"/>
	<rename from="sex" to="Gender"/>
	<renameSubMessages from="address" to="Address"/>
	<formatgender/>
	<flattenToFirst from="photos" to="Photo"/>
	<flattenToColumn from="Photo" to="Photo" property="data"/>
	<remove name="updateby"/>
	<remove name="identtype"/>
	<remove name="language"/>
	<remove name="title"/>
	<remove name="suffix"/>
	<remove name="nationality"/>
	<remove name="activeentity"/>
	<formatcommunication/>
</transform>